{
  "name": "webship/vdo",
  "description": "The Visual Distribution Operator (VDO) management system helps Drupal 8 distribution maintainers manage the base code and logic for the work cycle of development with number of workspaces for custom profiles, sub-profiles on local development machines or servers.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
  "issues": "http://drupal.org/project/issues/vdo",
  "source": "http://cgit.drupalcode.org/vdo"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.2",
    "drupal/core-composer-scaffold": "^8.8.0",
    "drupal/core-project-message": "^8.8.0",
    "drupal/core-recommended": "^8.8.0"
  },
  "require-dev": {
    "drupal/core-dev": "^8.8.0",
    "drush/drush": "^9.0"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev",
      "dev-9.x-1.x": "9.1.x-dev"
    },
    "installer-paths": {
      "web/core": ["type:drupal-core"],
      "web/libraries/{$name}": ["type:drupal-library"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/profiles/contrib/{$name}": ["type:drupal-profile"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
    }
  }
}