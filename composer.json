{
  "name": "webship/vdo",
  "description": "VDO Visual do helps Drupal 8 developers manage base code development work cycle for custom distributions/profiles on a local development, or servers.",
  "type": "drupal-profile",
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/vdo",
    "source": "http://cgit.drupalcode.org/vdo"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "composer/installers": "^1.0.24",
    "wikimedia/composer-merge-plugin": "^1.4",
    "cweagans/composer-patches": "^1.6.0",
    "drupal/drupal-library-installer-plugin": "~0.3",
    "drupal-composer/drupal-scaffold": "^2.0.0",
    "drupal/core": "8.4.0-alpha1",
    "drupal/console": "~1.0",
    "drush/drush": "^8.0.0",
    "drupal/libraries": "3.x-dev",
    "drupal/adminimal_theme": "1.3",
    "drupal/config_update": "1.3",
    "drupal/ctools": "3.0",
    "drupal/token": "1.0",
    "drupal/page_manager": "4.0-beta2",
    "drupal/panels": "4.2",
    "drupal/ds": "3.1",
    "drupal/field_group": "1.0-rc6",
    "drupal/advanced_text_formatter": "1.0-beta3",
    "drupal/block_class": "1.0-alpha1",
    "drupal/menu_block": "1.4",
    "drupal/admin_toolbar": "1.19",
    "drupal/adminimal_admin_toolbar": "1.5",
    "drupal/better_exposed_filters": "3.0-alpha3",
    "drupal/features": "3.5",
    "drupal/diff": "1.0-rc1",
    "drupal/devel": "1.0-rc2",
    "drupal/pathauto": "1.0",
    "drupal/entity": "1.0-alpha4",
    "drupal/entity_browser": "1.0",
    "drupal/super_login": "1.0",
    "drupal/machine_name": "1.x-dev",
    "drupal/hook_post_action": "1.0-beta1"
  },
  "require-dev": {
    "drupal/drupal-extension": "^3.2",
    "phing/phing": "^2.14",
    "drush/drush": "~9",
    "behat/behat": "^3.0",
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "behat/mink-zombie-driver": "@stable",
    "behat/mink-selenium2-driver": "~1.3",
    "behatch/contexts": "~2.7.1",
    "emuse/behat-html-formatter": "^0.1.0"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "autoload": {
    "psr-4": {
      "VDO\\composer\\": "src/composer"
    }
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": ["VDO\\composer\\Procedures::checkComposerVersionProcedure"],
    "pre-update-cmd": ["VDO\\composer\\Procedures::checkComposerVersionProcedure"],
    "post-install-cmd": [
      "VDO\\composer\\Procedures::createRequiredFilesProcedure",
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "VDO\\composer\\Procedures::createRequiredFilesProcedure",
      "./bin/phing push"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-paths": {
      "docroot/core": ["type:drupal-core"],
      "docroot/libraries/{$name}": ["type:drupal-library"],
      "docroot/modules/{$name}": ["type:drupal-module"],
      "docroot/profiles/{$name}": ["type:drupal-profile"],
      "docroot/themes/{$name}": ["type:drupal-theme"],
      "drush/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true
  }
}
