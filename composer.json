{
  "name": "webship/vdo",
  "description": "The Visual Distribution Operator (VDO) management system helps Drupal 8 distribution maintainers manage the base code and logic for the work cycle of development with number of workspaces for custom profiles, sub-profiles on local development machines or servers.",
  "type": "drupal-profile",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Webship",
      "homepage": "https://www.drupal.org/webship",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "http://drupal.org/project/issues/vdo",
    "source": "http://cgit.drupalcode.org/vdo"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
      },
    "assets": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  },
  "require": {
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "drupal-composer/drupal-scaffold": "^2.5",
    "webflo/drupal-finder": "^1.0.0",
    "webmozart/path-util": "^2.3",
    "drupal/core": "*",
    "drupal/config_provider": "^2.0",
    "drupal/config_snapshot": "^1.0",
    "drupal/config_merge": "^1.0",
    "drupal/config_sync": "^2.0",
    "drupal/config_distro": "^1.0",
    "drupal/config_update": "^1.0",
    "drupal/config_filter": "^1.0",
    "drupal/config_normalizer": "^1.0",
    "drupal/config_split": "^1.0",
    "drupal/config_ignore": "^2.0",
    "drupal/adminimal_theme": "^1.0",
    "drupal/admin_toolbar": "^1.0",
    "drupal/adminimal_admin_toolbar": "^1.0"
  },
  "require-dev": {
    "drupal/console": "^1.0.2",
    "drush/drush": "^9.0.0",
    "drupal/coder": "^8.0",
    "drupal/drupal-extension": "^3.0",
    "phing/phing": "^2.0",
    "behat/behat": "^3.0",
    "behat/mink": "^1.0",
    "behat/mink-extension": "^2.0",
    "behat/mink-goutte-driver": "^1.0",
    "behat/mink-zombie-driver": "^1.0",
    "behat/mink-selenium2-driver": "^1.0",
    "behatch/contexts": "^3.0",
    "emuse/behat-html-formatter": "^0.1"
  },
  "config": {
    "bin-dir": "bin/",
    "secure-http": false
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "@composer drupal-scaffold",
      "./bin/phing push"
    ],
    "post-update-cmd": [
      "./bin/phing push"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-1.x": "8.1.x-dev"
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "web/core": ["drupal/core"],
      "web/profiles/{$name}": ["type:drupal-profile"],
      "web/modules/contrib/{$name}": ["type:drupal-module"],
      "web/themes/contrib/{$name}": ["type:drupal-theme"],
      "web/libraries/{$name}": [
          "type:drupal-library",
          "type:bower-asset",
          "type:npm-asset"
      ],
      "web/modules/custom/{$name}": ["type:drupal-custom-module"],
      "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
      "drush/contrib/{$name}": ["type:drupal-drush"]
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    }
  }
}
